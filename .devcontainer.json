{
    "dockerComposeFile": [
        "./docker-compose.yaml"
    ],
    "service": "keria",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-azuretools.vscode-docker"
            ],
            "settings": {
                "python.terminal.activateEnvInCurrentTerminal": true,
                "python.defaultInterpreterPath": ".venv/bin/python"
            }
        }
    },
    "workspaceFolder": "/workspace",
    "postCreateCommand": "./scripts/install.sh"
}
